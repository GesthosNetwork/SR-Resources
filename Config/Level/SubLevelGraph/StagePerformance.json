                  {
                    "$type": "RPG.GameCore.ByTargetTeam",
                    "TargetType": {
                      "$type": "RPG.GameCore.TargetAlias",
                      "Alias": "CurrentTurnOwnerEntity"
                    },
                    "Team": "TeamLight"
