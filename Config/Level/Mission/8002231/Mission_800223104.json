        },
        {
          "$type": "RPG.GameCore.SetLocalPlayerDitherAlpha",
          "DitherAlpha": 1,
          "Duration": 0.2
        },
        {
          "$type": "RPG.GameCore.ActiveVirtualCamera",
          "AreaName": "UseInteractNPC",
