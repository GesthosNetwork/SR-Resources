{
  "OnInitSequece": [],
  "OnStartSequece": [
    {
      "IsLoop": true,
      "TaskList": [
        {
          "$type": "RPG.GameCore.WaitCustomString",
          "CustomString": {
            "Custom": true,
            "Key": "InitPuzzle_Str"
          },
          "GoNextImmediately": true
        },
        {
          "$type": "RPG.GameCore.SetTrackingMission"
        }
      ]
    },
    {
      "IsLoop": true,
      "TaskList": [
        {
          "$type": "RPG.GameCore.WaitCustomString",
          "CustomString": {
            "Custom": true,
            "Key": "ClosePuzzle_Str"
          },
          "GoNextImmediately": true
        },
        {
          "$type": "RPG.GameCore.RestoreTrackingMission"
        }
      ]
    }
  ],
  "ValueSource": {
    "Values": [
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "InitPuzzle_Str",
        "Comment": "从外部开启玩法"
      },
      {
        "$type": "RPG.GameCore.SharedString",
        "Key": "ClosePuzzle_Str",
        "Comment": "从外部关闭玩法"
      }
    ]
  }
}